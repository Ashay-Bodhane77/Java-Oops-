public class Linear Search {

public static void main(String[] args) {

int[] arr = {10, 25, 30, 45, 50};

int key = 30; // element to search

int result = linear Search(arr, key);

if (result == -1)

System.out.println("Element not

found");

else

System.out.println("Element found

at index: + result);

}

// Linear search method

public static int linear Search(int[] arr,

int key) {

{

for (int i = 0; i < arr.length; i++)

if (arr[i] == key) {

element found

}

}

return i; // return index if

return -1; // return -1 if element

not found

}

}
