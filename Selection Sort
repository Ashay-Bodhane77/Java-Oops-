public class SelectionSort {

int []arr={5,1,4,3,2);

public void sort(){

for(int i=0;i<arr.length-1;i++){

int small=i;

} for(int j=i+1;j<arr.length;j++){ if(arr[j]<arr[small]){ small=j; } } }

//swap

int temp=arr[small];

arr [small]=arr[i];

arr[i] = temp;

} //print

public void print(){ for(int i=0;i<arr.length;i++){ System.out.print(arr[i]+" ");

public static void main(String[] args) {

SelectionSort s= new SelectionSort ();

} }

s.sort();

s.print();

}
